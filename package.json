{
  "name": "spielbetrieb",
  "version": "1.0",
  "description": "spielbetrieb.eu webpage",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vanStaen/spielbetrieb.eu.git"
  },
  "scripts": {
    "start": "node server/index.js",
    "devfr": "webpack serve --config webpack.config.dev.cjs --hot --open",
    "dev": "nodemon server/index.js",
    "prebuild": "rm -rf ./build",
    "build": "webpack --config webpack.config.prod.cjs --mode production",
    "predeploy": "npm run build",
    "deploy": "npm i; heroku git:remote -a spielbetrieb; git push heroku master; npm run backup;",
    "postdeploy": "rm -rf ./build",
    "log": "heroku logs --tail",
    "version": "heroku --version; git --version",
    "kill": "killall -9 node",
    "backup": "cd server/lib/; node createDatabaseBackUpFile.js; npm run pushbackup; cd ../../",
    "pushbackup": "cd ../database-backups; git pull origin master; git add -A; git commit -m 'Spielbetrieb - database backup files'; git push;",
    "lint": "eslint -c ./.eslintrc.json '**/*.js' '**/*.jsx' --fix",
    "scraper": "npm run scrapekitty; npm run scrapestolzdemut; npm run scrapeinso; npm run scrapeklubverboten",
    "scrapeinso": "cd server/lib/scrapers; node insomnia.js/; cd ../../../",
    "scrapekitty": "cd server/lib/scrapers; node kitkatclub.js/; cd ../../../",
    "scrapestolzdemut": "cd server/lib/scrapers; node stolzdemut.js/; cd ../../../",
    "scrapeklubverboten": "cd server/lib/scrapers; node klubverboten.js/; cd ../../../"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.533.0",
    "@aws-sdk/s3-request-presigner": "^3.540.0",
    "@babel/eslint-parser": "^7.24.1",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@sendgrid/mail": "^8.1.1",
    "antd": "^5.14.0",
    "axios": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "html2canvas": "^1.4.1",
    "i18next": "^23.2.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "i18next-http-backend": "^2.5.1",
    "jsonwebtoken": "^9.0.2",
    "lodash.isequal": "^4.5.0",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "multer": "^1.4.5-lts.1",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "puppeteer": "^22.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.0.0",
    "react-router-dom": "^6.13.0",
    "regenerator-runtime": "^0.13.11",
    "sequelize": "^6.32.1",
    "sharp": "^0.32",
    "universal-cookie": "^4.0.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.31.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "file-loader": "^6.2.0",
    "forever": "^4.0.3",
    "html-webpack-plugin": "^5.5.3",
    "less": "^4.1.3",
    "less-loader": "^11.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^3.2.5",
    "sass": "^1.64.0",
    "sass-loader": "^13.3.2",
    "sequelize-cli": "^6.2.0",
    "style-loader": "^3.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "type": "module"
}